{
  "kind": "collectionType",
  "collectionName": "videos",
  "info": {
    "singularName": "video",
    "pluralName": "videos",
    "displayName": "Video"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "youtube_url": {
      "type": "string",
      "required": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "allowedTypes": ["images"]
    },
    "description": {
      "type": "text"
    },
    "published_date": {
      "type": "date"
    },
    "related_industries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::industry.industry"
    }
  }
}
